{
  "author": "mattheri",
  "description": "A package to generate route references for React Router 7",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@changesets/cli": "^2.29.4",
    "@react-router/dev": "^7.6.0",
    "@types/node": "^22.15.17",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react-router-7",
    "routes"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "generate-routes-refs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattheri/generate-routes-refs.git"
  },
  "scripts": {
    "build": "tsc",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "npm run build && npm run check-exports && npm run test",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci",
    "test": "vitest run"
  },
  "type": "module",
  "version": "0.0.3",
  "dependencies": {
    "upath": "^2.0.1"
  }
}
